<% layout('layouts/boilerplate') -%>
    <div>
        <a href="/campgrounds/new">Add new campground</a>
    </div>
    <ul>
        <% for (let campground of campGrounds){ %>
            <li><a href="/campgrounds/<%=campground._id%>">
                    <%= campground.title %>
                </a></li>
            <% } %>
    </ul>